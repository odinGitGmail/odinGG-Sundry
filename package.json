{
  "name": "odingg-sundry",
  "displayName": "odingg-sundry",
  "description": "",
  "version": "1.0.1",
  "icon": "assets/avatar.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odinGitGmail/odinGG-Sundry.git"
  },
  "publisher": "odinsam",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/odinGitGmail/odinGG-Sundry/issues"
  },
  "homepage": "https://github.com/odinGitGmail/odinGG-Sundry#readme",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "odin-sundry.generateGuidToUpper",
        "key": "ctrl+shift+g ctrl+shift+u",
        "mac": "cmd+shift+g cmd+shift+u"
	    },
      {
        "command": "odin-sundry.generateGuidToLower",
        "key": "ctrl+shift+g ctrl+shift+l",
        "mac": "cmd+shift+g cmd+shift+l"
      }
    ],
    "commands": [
      {
        "command": "odin-sundry.generateGuidToUpper",
        "title": "大写"
      },
      {
        "command": "odin-sundry.generateGuidToLower",
        "title": "小写"
      },
      {
        "command": "odin-sundry.generateGitgnore",
        "title": "生成 .gitgnore"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "SundryExplorerContext",
          "group": "z_command@1"
        }
      ],
      "SundryExplorerContext":[
        {
          "command": "odin-sundry.generateGitgnore",
          "group": "sundryEditorContext@1"
        }
      ],
      "editor/context": [
        {
          "group": "z_command@1",
          "submenu": "SundryEditorContext"
        }
      ],
      "SundryEditorContext":[
        {
          "submenu": "GenerateGuid"
        }
      ],
      "GenerateGuid":[
        {
          "command": "odin-sundry.generateGuidToUpper",
          "group": "sundry_generateGuid@1"
        },
        {
          "command": "odin-sundry.generateGuidToLower",
          "group": "sundry_generateGuid@2"
        }
      ]
    },
    "submenus": [
      {
        "id": "SundryEditorContext",
        "label": "小工具"
      },
      {
        "id": "GenerateGuid",
        "label": "生成Guid"
      },
      {
        "id": "SundryExplorerContext",
        "label": "小工具"
      }
    ]
  },
  "scripts": {
    "release": "standard-version",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
